<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=System"
                    xmlns:conv="clr-namespace:Editor.Core.Wpf.Converters">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Colors.xaml"/>
        <ResourceDictionary>
            <sys:Boolean x:Key="True">true</sys:Boolean>
            <sys:Boolean x:Key="False">false</sys:Boolean>
            <sys:Double x:Key="Unit">32</sys:Double>
            <sys:Double x:Key="StrokeThickness">2</sys:Double>
            <conv:ColorToBrushConverter x:Key="ColorToBrushConverter">
                <conv:ColorToBrushConverter.ResourceDictionary>
                    <ResourceDictionary Source="Colors.xaml"/>
                </conv:ColorToBrushConverter.ResourceDictionary>
            </conv:ColorToBrushConverter>
        </ResourceDictionary>
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>