<UserControl x:Class="Editor.Core.Wpf.View.EditorEntitiesListItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Editor.Core.Wpf.View"
             xmlns:vm="clr-namespace:Editor.Core.Wpf.ViewModel"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             d:DataContext="{d:DesignInstance vm:EditorEntitiesListItemViewModel}">
    <Image
        Width="32"
        Height="32"
        Margin="16"
        Source="{Binding Image}"/>
</UserControl>
