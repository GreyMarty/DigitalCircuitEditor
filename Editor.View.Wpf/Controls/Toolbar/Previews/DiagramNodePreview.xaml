             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Editor.View.Wpf.Controls.Toolbar.Previews"
             mc:Ignorable="d"
             d:DesignWidth="96" d:DesignHeight="96">
